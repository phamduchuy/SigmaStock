<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="admin.sigmastock.sigmastock.Fragment.FmHome">
    <LinearLayout
        android:gravity="center"
        android:layout_marginTop="10dp"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <ProgressBar
            android:indeterminateTint="@color/color_Green_Material"
            android:id="@+id/pbTopProgress"
            android:visibility="visible"
            android:layout_width="30dp"
            android:layout_height="30dp" >
        </ProgressBar>
    </LinearLayout>
    <ExpandableListView
        android:id="@+id/listView"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:childDivider="@drawable/dividers"
        android:divider="@drawable/dividers"
        android:groupIndicator="@null"
        >
    </ExpandableListView>

</FrameLayout>
